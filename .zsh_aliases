alias pnxu='pnpm nuxi upgrade --force' # Nuxi Upgrade and Fix .nuxt & node_modules

# Nuxt CLI Tool Aliases
alias nx='nuxi' # Nuxt CLI Tool
alias nxc='nuxi cleanup' # Nuxi Cleanup generated nuxt files and caches
alias nxu='nuxi upgrade --force' # Nuxi Upgrade and Fix .nuxt & node_modules
